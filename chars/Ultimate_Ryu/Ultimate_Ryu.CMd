;---------------------------------------
;guarding AI
[State -1, Stand guard AI]
type = ChangeState
value = 130
triggerall = Var(59)>=1&& StateType != A && Ctrl&& EnemyNear, Facing != Facing
trigger1 = P2StateType != C && P2MoveType = A && ((p2bodydist X <= 100) || (EnemyNear, NumProj > 0))
trigger1 = random <= 500
persistent = 0

;guarding Crouch AI
[State -1, crouch guard2 AI]
type = ChangeState
value = 131
triggerall = Var(59)>=1&& StateType != A && Ctrl && EnemyNear, Facing != Facing
trigger1 = P2StateType = C && P2MoveType = A && ((p2bodydist X <= 100) || (EnemyNear, NumProj > 0))
trigger1 = random <= 500
persistent = 0

;Air guarding AI
[State -1, Air guard3 AI]
type = ChangeState
value = 132
triggerall = Var(59)>=1 && StateType = A && Ctrl && EnemyNear, Facing != Facing
trigger1 = P2MoveType = A && ((p2bodydist X <= 100) || (EnemyNear, NumProj > 0))
trigger1 = random <= 500
value = 132
